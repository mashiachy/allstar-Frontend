include mixins

doctype html
html(lang="us")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name="robots" content="noindex, nofollow")
    title AllStars | 404
    link(rel="preload" href="css/404.css" as="style")
    link(rel="preload" href="img/404-number.svg" as="image")
    link(rel="preload" href="img/404-sun.svg" as="image")
    link(rel="preload" href="img/404-back.svg" as="image")
    link(rel="preload" href="img/404-oval.svg" as="image")
    link(rel="preload" href="img/404-bottom.svg" as="image")
    link(rel="preload" href="img/404-subbottom.svg" as="image")
    link(href="css/404.css" rel="stylesheet")
  body
    +b.SECTION.main
      +e.A(href='#').logo-link
        +e.IMG(src='img/Logo.svg' alt='Logo').logo
      +e.description Page not found....
      +e.A(href='#').button Home
    +b.image-404
      +e.number
      +e.sun
      +e.back
      +e.oval
      +e.bottom
      +e.subbottom

    script(src="js/404.js")